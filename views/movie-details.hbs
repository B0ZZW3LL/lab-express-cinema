 <div>
    <div>
        <img src="{{movie.image}}" alt="">
    </div>
    
    <div id="movie-details">
        <a href="/movies">Go back</a>
        <h4>{{movie.title}}</h4>
        <h5>Dirctor: {{movie.director}}</h5>
        <ul>
            {{#each movie.stars }}
                <li>{{this}}</li>
            {{/each}}
        </ul>
        <p>{{movie.description}}</p>
        <p>{{movie.showtimes}}</p>
    </div>
 </div>
















